{
  "name": "nw-react",
  "version": "0.0.0",
  "author": "andrew",
  "license": "ISC",
  "main": "js/boot.js",
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d .",
    "build": "NODE_ENV=production browserify . | uglifyjs -cm > js/bundle.min.js",
    "test": "jest"
  },
  "dependencies": {
    "flux": "~2.0.1",
    "react": "~0.12.0",
    "q": "~1.0.1",
    "highcharts-browserify": "~0.1.1-4.0.3",
    "react-router": "~0.11.0",
    "moment": "~2.8.4",
    "jquery": "~2.1.1",
    "noty": "~2.3.2",
    "extend": "~2.0.0",
    "numeral": "~1.5.3",
    "moment-duration-format": "~1.3.0",
    "react-bootstrap": "~0.13.0"
  },
  "devDependencies": {
    "browserify": "~6.2.0",
    "gulp": "~3.8.10",
    "envify": "~3.0.0",
    "uglify-js": "~2.4.15",
    "reactify": "~0.15.2",
    "watchify": "~2.1.0",
    "express": "~4.8.8",
    "express3-handlebars": "~0.5.2",
    "vinyl-source-stream": "~1.0.0",
    "gulp-util": "~3.0.1",
    "gulp-notify": "~2.0.1",
    "jquery-browserify": "~1.8.1",
    "browserify-shim": "~3.8.0",
    "gulp-watch": "~2.0.0",
    "morgan": "~1.3.0",
    "gulp-inject": "~1.0.2",
    "gulp-rename": "~1.2.0",
    "gulp-react": "~2.0.0"
  },
  "browser": {
    "d3": "./bower_components/d3/d3.min.js",
    "jquery": "./bower_components/jquery/dist/jquery.min.js",
    "nvd3": "./bower_components/nvd3/nv.d3.min.js",
    "highcharts": "./bower_components/highcharts/highcharts.src.js",
    "highchartsAdapter": "./bower_components/highcharts/adapters/standalone-framework.src.js",
    "bootstrap": "./bower_components/bootstrap/dist/js/bootstrap.js",
    "noty": "./node_modules/noty/js/noty/packaged/jquery.noty.packaged.js",
    "chart": "./bower_components/chartjs/Chart.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "noty": "noty",
    "chart": "chart",
    "highcharts": {
      "depends": [
        "highchartsAdapter:highchartsAdapter"
      ],
      "exports": "highcharts"
    },
    "highchartsAdapter": {
      "exports": "highchartsAdapter"
    },
    "d3": {
      "exports": "d3",
      "depends": [
        "jquery:$"
      ]
    },
    "bootstrap": {
      "exports": "bootstrap",
      "depends": {
        "jquery": "$"
      }
    },
    "nvd3": {
      "exports": "nv",
      "depends": [
        "jquery:$",
        "d3:d3"
      ]
    }
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
